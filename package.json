{
  "name": "aurelia-node-templating-engine",
  "description": "A tool to render view/view-models using Aurelia (https://aurelia.io) from node!",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "example": "ts-node examples/example.ts",
    "example:minify-options": "ts-node examples/example-minify-options.ts",
    "prebuild": "rimraf dist && tslint --project tsconfig.json",
    "build": "tsc -b"
  },
  "main": "dist/build/index.js",
  "typings": "dist/index.d.ts",
  "dependencies": {
    "aurelia-framework": "^1.3.1",
    "aurelia-loader-nodejs": "^1.0.1",
    "aurelia-pal-nodejs": "^1.2.0",
    "aurelia-polyfills": "^1.3.1",
    "aurelia-templating": "^1.10.1",
    "aurelia-templating-binding": "^1.5.2",
    "aurelia-templating-resources": "^1.7.2",
    "html-minifier": "^3.5.21"
  },
  "devDependencies": {
    "@types/html-minifier": "^3.5.2",
    "@types/node": "^10.12.18",
    "typescript": "^3.2.4",
    "tslint": "^5.12.1",
    "ts-node": "^7.0.1",
    "rimraf": "^2.6.3"
  },
  "engines": {
    "node": ">= 6.0.0"
  }
}